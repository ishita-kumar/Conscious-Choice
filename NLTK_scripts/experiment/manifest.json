{
    "name": "Experiment1",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
      "browser_action": {
        "default_title": "experiment"
      },
      "background": {
        "scripts": ["background.js"]
      },
      "permissions": ["tabs"]
    }